    <div class="container">
        <h2>Form control: input</h2>
        <p>The form below contains two input elements; one of type text and one of type password:</p>
        <form class="form" method="post", action="/actualizar/<%= data[0].noSerie %>" >
            <input(type="hidden", name="_method", value="PUT">
        
          <div class="form-group">
            <label for="usr">Numero de Serie:</label>
            <input class="form-control" type="text" name="noCliente"  value=<%= data[0].noCliente%>  >
          </div>
          <div class="form-group">
            <label for="pwd">Marca:</label>
            <input class="form-control" type="text" name="aPaterno" placeholder="aPaterno"  value=<%= data[0].aPaterno%>  required="required" />
		  </div>
		  <div class="form-group">
            <label for="pwd">Tipo:</label>
            <input class="form-control" type="text" name="aMaterno" placeholder="aMaterno" value=<%= data[0].aMaterno%> required="required" , >
		  </div>
		  <div class="form-group">
            <label for="pwd">Transmision:</label>
            <input  class="form-control" type="text" name="nombres" placeholder="nombres"  value=<%= data[0].nombres%> required="required", >
		  </div>
		  <div class="form-group">
            <label for="pwd">Puertas:</label>
            <input class="form-control" type="text" name="domicilio" placeholder="domicilio" value=<%= data[0].domicilio%> required="required">
		  </div>
		  <div class="form-group">
            <label for="pwd">Fecha de Introduccion:</label>
            <input class="form-control" type="date" name="fechaNac" placeholder="Fecha de Nacimiento"   data-date-format="yyyy/mm/dd" value=<%= data[0].fechaNac%>, required="required">
		  </div>
		  <div><input class="button edit" type="submit" value="Editar"/></div>
        </form>
      </div>
      

